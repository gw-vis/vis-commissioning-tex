@article{Mueller:05,
	author = {Guido Mueller},
	journal = {Opt. Express},
	keywords = {Fabry-Perot; Interferometry; Astronomical optics; Optical engineering; Beam splitters; Detectors; Laser beams; Optical components; Phase shift; Photodetectors},
	number = {18},
	pages = {7118--7132},
	publisher = {OSA},
	title = "{Beam jitter coupling in advanced LIGO}",
	volume = {13},
	month = {Sep},
	year = {2005},
	url = {http://www.opticsexpress.org/abstract.cfm?URI=oe-13-18-7118},
	doi = {10.1364/OPEX.13.007118},
	abstract = {Fluctuations in the position or propagation direction of the laser beam (beam jitter) is one of the most critical technical noise sources in an interferometric gravitational wave detector. These fluctuations couple to spurious misalignments of the mirrors forming the interferometer and potentially decrease the sensitivity. In this paper we calculate the transfer function of beam jitter into the gravitational wave channel for the Advanced LIGO detector and derive a first expression for the requirements on beam jitter for Advanced LIGO.},
}

